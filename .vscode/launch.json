{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Go Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/goserver"
        },
        {
            "name": "Python Client Demo",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/py/demo.py",
            "console": "integratedTerminal"
        },
    ]
}